{
  "name": "libi-back",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json && npm run copy-prompts",
    "copy-prompts": "mkdir -p dist/modules/ai/prompts && cp src/modules/ai/prompts/*.md dist/modules/ai/prompts/",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "test": "vitest",
    "test:watch": "vitest watch",
    "prisma:generate": "prisma generate",
    "prisma:dev": "prisma migrate dev --name init",
    "prisma:migrate": "prisma migrate deploy",
    "cli": "tsx src/cli/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/commander": "^2.12.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
